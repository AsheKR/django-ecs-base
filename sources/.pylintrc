[MASTER]
load-plugins=pylint_django
ignore=migrations,settings

[FORMAT]
max-line-length=120

[MESSAGES CONTROL]
disable=missing-docstring,invalid-name,import-error,no-self-use,import-error,too-few-public-methods,django-not-available

[DESIGN]
max-parents=13

[TYPECHECK]
generated-members=REQUEST,acl_users,aq_parent,"[a-zA-Z]+_set{1,2}",save,delete